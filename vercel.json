{
  "version": 2,
  "builds": [
    {
      "src": "lovebot_interface.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.jpg", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.jpg$)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/lovebot_interface.html"
    }
  ]
}
